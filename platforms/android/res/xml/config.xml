<?xml version='1.0' encoding='utf-8'?>
<widget id="co.uk.Tuxedo" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Hello Tuxedo</name>
    <description>
        A sample Tuxedo App
    </description>
    <access origin="*" />
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <author email="caiaoricci@gmail.com" href="http://mintify.org">
        Mintify
    </author>
    <content src="tuxedo.html" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
</widget>
